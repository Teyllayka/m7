{
    "builds": [
      {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "mysite/wsgi.py"
      }
    ],
    "buildCommand": "python manage.py collectstatic --noinput"
  }
  